{
  "name": "diastema-web-app",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon -L app.js",
    "dockerup": "docker-compose --project-directory ./  -f ./docker/docker-compose.dev.yml up -d",
    "dockerdown": "docker-compose --project-directory ./  -f ./docker/docker-compose.dev.yml down",
    "dockerprod": "docker-compose --project-directory ./  -f ./docker/docker-compose.prod.yml up -d",
    "dockerdownprod": "docker-compose --project-directory ./  -f ./docker/docker-compose.prod.yml down",
    "dockerbuild": "docker-compose --project-directory ./  -f ./docker/docker-compose.dev.yml up -d --build",
    "pushall": "git add . && git commit -m \"general updates\" && git push dev master",
    "pushreadme": "git add README.md && git commit -m \"Update README.md\" && git push dev master"
  },
  "keywords": [],
  "author": "Andreas Karabetian",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^2.0.16"
  },
  "dependencies": {
    "body-parser": "^1.20.0",
    "connect-mongo": "^4.6.0",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "mongodb": "^4.6.0",
    "mongoose": "^6.3.4",
    "multer": "^1.4.4",
    "node-fetch": "^2.6.5",
    "socket.io": "^4.5.1"
  }
}
