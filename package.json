{
  "name": "diastema-web-app",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon -L app.js",
    "docker:dev": "docker-compose --project-directory ./  -f ./docker/docker-compose.dev.yml up -d",
    "docker:downdev": "docker-compose --project-directory ./  -f ./docker/docker-compose.dev.yml down",
    "docker:prod": "docker-compose --project-directory ./  -f ./docker/docker-compose.prod.yml up -d",
    "docker:downprod": "docker-compose --project-directory ./  -f ./docker/docker-compose.prod.yml down",
    "docker:build": "docker-compose --project-directory ./  -f ./docker/docker-compose.dev.yml up -d --build",
    "push:all": "git add . && git commit -m \"general updates\" && git push origin master",
    "push:readme": "git add README.md && git commit -m \"Update README.md\" && git push origin master"
  },
  "keywords": [],
  "author": "Andreas Karabetian",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^2.0.16"
  },
  "dependencies": {
    "body-parser": "^1.20.0",
    "connect-mongo": "^4.6.0",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "minio": "^7.0.32",
    "mongodb": "^4.6.0",
    "mongoose": "^6.3.4",
    "multer": "^1.4.4",
    "node-fetch": "^2.6.5",
    "socket.io": "^4.5.1"
  }
}
